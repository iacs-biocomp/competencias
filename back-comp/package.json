{
	"name": "back-comp",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"build:compiled": "nest build && ncc build -m dist/back-comp/src/main.js && rm -rf dist/back-comp dist/sharedCode ",
		"build-webpack": "webpack",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "ts-node src/index.ts",
		"start:dev": "rimraf dist && nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "NODE_ENV=production node dist/back-comp/src/main",
		"start:prod_compiled": "NODE_ENV=production node dist/index",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"migration:generate": "npm run typeorm -- migration:generate --config ./orm.config.json --name",
		"migration:run": "npm run typeorm -- migration:run --config ./orm.config.json",
		"migration:revert": "npm run typeorm -- migration:revert --config ./orm.config.json",
		"typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
		"nest": "nest"
	},
	"dependencies": {
		"@aml360/nestjs-compression": "6.0.103",
