		"@types/node": "17.0.8",
		"@types/rellax": "1.7.4",
		"@typescript-eslint/eslint-plugin": "5.9.1",
		"@typescript-eslint/parser": "5.9.1",
		"commitizen": "4.2.4",
		"cz-conventional-changelog": "3.3.0",
